  <!-- FORM SECTION START -->
  <h2 class="text-center">Upload New Assignment</h2>
  <br>


 <!-- code for alert starts -->
<% if(message!="") {
  %>
  <div class="alert alert-success alert-dismissible">
    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
     <strong></strong><%= message %>
  </div>

<%}%>

<!-- code for elert ends -->


  <br>
<form class="form-horizontal"  method="POST" action="submit_assignment" enctype="multipart/form-data">
  <div class="col-sm-6">
    <div class="form-group">
      <label class="control-label col-sm-4">Your Name</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" name="name">
      </div>
    </div>


     <div class="form-group">
      <label class="control-label col-sm-4">Your Highest Digree</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" name="degree">
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="form-group">
      <label class="control-label col-sm-4">Your Email ID</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" name="email">
      </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-4">Your Mobile</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" name="mobile">
      </div>
    </div>
  </div>

  <div class="col-sm-12">
    <div class="form-group">
      <label class="control-label col-sm-2">Your Address</label>
      <div class="col-sm-10">
        <textarea type="text" class="form-control" rows="2" name="address"></textarea>
      </div>
    </div>
  </div>


  <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">Subject</label>
      <div class="col-sm-8">
        <input type="text" class="date-start ml-5 form-control datepicker" placeholder="ie. English Math etc" name="subject">
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">Chapter Name</label>
      <div class="col-sm-8">
        <input type="text" class="date-end ml-5 form-control datepicker col-sm-8" placeholder="as given in book" name="chapter">
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">Topic Name</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" name="topic">
      </div>
    </div>
  </div>
  
  <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">No Of Questions</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" name="number_of_ques">
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">Yutube Channel Name</label>
    <div class="col-sm-8">
        <input type="text" class="form-control" name="channel">
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">Link Of playlist</label>
    <div class="col-sm-8">
        <input type="text" class="form-control" name="link_of_playlist">
      </div>
    </div>
  </div>
   <div class="col-sm-6">
    <div class="form-group">
      <label class="col-sm-4 control-label">Drive link</label>
    <div class="col-sm-8">
        <input type="text" class="form-control" name="drive_link">
      </div>
    </div>
  </div>


